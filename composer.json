{
    "name": "meltir/imdb-ratings-scraper",
    "description": "Scrape an imdb users reviews and ratings from their public imdb profile. This product uses the TMDB API but is not endorsed or certified by TMDB.",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Meltir\\ImdbRatingsScraper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Lukasz Andrzejak",
            "email": "packagist@meltir.com",
            "homepage": "https://www.meltir.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "symfony/dom-crawler": "^6.1",
        "symfony/css-selector": "^6.1",
        "symfony/event-dispatcher": "^6.1",
        "php-tmdb/api": "^4.0"
    }
}
